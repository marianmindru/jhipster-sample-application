<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Tranzactii.
    -->
    <changeSet id="20210823145053-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="oferta_id"
                                 baseTableName="tranzactii"
                                 constraintName="fk_tranzactii__oferta_id"
                                 referencedColumnNames="id"
                                 referencedTableName="oferte"/>

        <addForeignKeyConstraint baseColumnNames="polita_id"
                                 baseTableName="tranzactii"
                                 constraintName="fk_tranzactii__polita_id"
                                 referencedColumnNames="id"
                                 referencedTableName="polite"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="tranzactii"
                                 constraintName="fk_tranzactii__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="users"/>
    </changeSet>
</databaseChangeLog>
